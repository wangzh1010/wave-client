{
  "name": "wave-client",
  "productName": "wave-client",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "wangzheng",
    "email": "wangzheng@mail.china.cn"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "wave_client"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./views/index.html",
                  "js": "./render/renderer.js",
                  "name": "main_window"
                },
                {
                  "html": "./views/register.html",
                  "js": "./render/register.js",
                  "name": "register_window"
                },
                {
                  "html": "./views/userinfo.html",
                  "js": "./render/userinfo.js",
                  "name": "userinfo_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.51",
    "@electron-forge/maker-deb": "^6.0.0-beta.51",
    "@electron-forge/maker-rpm": "^6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.51",
    "@electron-forge/maker-zip": "^6.0.0-beta.51",
    "@electron-forge/plugin-webpack": "^6.0.4",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "css-loader": "^6.7.3",
    "electron": "9.0.4",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "vue-loader": "^17.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "view-design": "^4.2.0",
    "vue": "^2.6.11"
  }
}
